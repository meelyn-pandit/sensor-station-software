{
  "name": "sensor-station-software",
  "version": "1.0.5",
  "description": "monorepo for CTT Sensor Station Software",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "transpile-watch": "babel src -d lib --watch --copy-files",
    "build": "rm -rf lib && babel src -d lib --copy-files",
    "start-hardware-server": "node ./src/station-hardware-server/bin/www.js",
    "start-radio-interface": "node ./src/station-radio-interface/index.js",
    "start-lcd-interface": "node ./src/station-lcd-interface/index.js",
    "start-web-interface": "node ./src/station-interface/bin/www.js",
    "read-v3-serial": "node ./src/id-driver/serial-chip.js",
    "get-version": "node ./src/id-driver/get-version.js",
    "test-expander": "node ./src/io-expander/test/test.js",
    "test-adc": "node ./src/adc-driver/test/max11645.js",
    "test-leds": "node ./src/led-driver/test/index.js",
    "test-id": "node ./src/id-driver/v3-driver.js",
    "test-revision": "node ./src/id-driver/station-revision.js",
    "test-usb-scanner": "node ./src/usb-storage-driver/test/index.js"
