    "ncp": "^2.0.0",
    "node-fetch": "^2.6.7",
    "node-gpsd": "^0.3.2",
    "onoff": "^6.0.3",
    "pug": "^3.0.2",
    "serialport": "^7.1.5",
    "sleep": "^6.3.0",
    "tail": "^2.2.4",
    "tar": "^6.1.11",
    "usb": "^2.3.1",
